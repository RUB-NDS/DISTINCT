{
	"manifest_version": 2,
	"name": "chrome-extension",
	"version": "1",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"all_frames": true,
			"run_at": "document_start",
			"js": [
				"helpers.js",
				"detect_frame.js",
				"detect_communication.js",
				"detect_storage.js",
				"detect_sdks.js",
				"detect_flow.js"
			]
		}
	],
	"permissions": [
		"webRequest", "webRequestBlocking", "http://*/*", "https://*/*"
	],
	"background": {
		"scripts": [
			"disable_csp.js"
		],
		"persistent": true
	}
}