{
	"manifest_version": 2,
	"name": "chrome-extension",
	"version": "1",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["helpers.js", "detect_frame.js", "detect_sdks.js", "detect_flow.js"],
			"all_frames": true,
			"run_at": "document_start"
		}
	],
	"permissions": ["webRequest", "webRequestBlocking", "http://*/*", "https://*/*"],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	}
}